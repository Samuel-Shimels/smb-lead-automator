{
  "name": "smb-lead-automator",
  "version": "1.0.0",
  "description": "Modern Google Apps Script-based lead generation application that integrates with Apollo.io API",
  "main": "deploy.js",
  "scripts": {
    "deploy": "node deploy.js",
    "push": "clasp push",
    "pull": "clasp pull",
    "open": "clasp open",
    "logs": "clasp logs",
    "setup": "clasp login && clasp create --type sheets --title 'SMB Leads Generation'"
  },
  "keywords": [
    "google-apps-script",
    "apollo-io",
    "lead-generation",
    "crm",
    "automation",
    "google-sheets"
  ],
  "author": "SMB Leads Team",
  "license": "MIT",
  "devDependencies": {
    "@google/clasp": "^2.4.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/smb-lead-automator.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/smb-lead-automator/issues"
  },
